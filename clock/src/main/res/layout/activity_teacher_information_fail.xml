<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:gravity="center_horizontal"
    android:orientation="vertical"
    tools:ignore="HardcodedText">

    <include layout="@layout/include_head_green_bg_left_icon_right_text_layout" />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/DIMEN_70DP"
        android:drawableLeft="@drawable/order_error_icon"
        android:drawablePadding="@dimen/DIMEN_10DP"
        android:text="@string/teacher_content_one"
        android:textColor="@color/black"
        android:textSize="@dimen/DIMEN_16DP" />

    <TextView
        android:gravity="center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/DIMEN_44DP"
        android:text="@string/teacher_content_two"
        android:textColor="@color/black"
        android:textSize="@dimen/DIMEN_17DP" />


</LinearLayout>
