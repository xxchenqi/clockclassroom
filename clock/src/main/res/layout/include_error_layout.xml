<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_gravity="center_horizontal"
    android:layout_marginTop="@dimen/DIMEN_50DP"
    android:orientation="horizontal"
    tools:ignore="HardcodedText">

    <Button
        android:id="@+id/bt_backindex"
        style="@style/style_bt_back_order"
        android:background="@drawable/background_green_1eb482_radius_2"
        android:text="重新预订"
        android:textColor="@color/white" />

    <Button
        android:id="@+id/bt_again_choose"
        style="@style/style_bt_back_order"
        android:layout_marginLeft="@dimen/DIMEN_12DP"
        android:background="@drawable/background_green_1eb482_stroke_radius_2"
        android:text="@string/check_order" />

</LinearLayout>