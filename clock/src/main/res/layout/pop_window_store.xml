<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_gay_f5"
    android:orientation="horizontal">

    <ListView
        android:id="@+id/list_pop_left"
        android:layout_width="@dimen/DIMEN_106DP"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:divider="@color/color_gay_ee"
        android:dividerHeight="@dimen/DIMEN_1DP"/>
    <ListView
        android:id="@+id/list_pop_right"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/color_gay_f5"
        android:divider="@color/color_gay_ee"
        android:dividerHeight="@dimen/DIMEN_1DP"/>

</LinearLayout>